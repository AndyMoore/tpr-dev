project: tpr
environments:
  - name: Development
    namespace: tpr-dev
    deployment:
      method: rolling-upgrade
    services:
      - type: mysql
        name: tprdevmysql
        version: 5.6
      - type: mongo
        name: tprdevmongo
        version: 2.6
        test: "abc"
      - type: mysql
        name: tprdevmysql2
        version: 5.6
        iops: 25
        dbinstanceclass: db.m3.large
        allocatedstorage: 200
