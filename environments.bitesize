project: tpr
environments:
  - name: Development
    namespace: tpr-dev
    deployment:
      method: rolling-upgrade
    services:
      - type: mysql
        name: tprdevmysql
        version: 5.6
      - type: mongo
        name: tprdevmongo
        version: 2.6
        test: "abc"
      - type: mysql
        name: tprdevmysql5
        version: 5.7
        replicas: 3
        options:
          multiaz: false
          dbinstanceclass: db.m3.large
          allocatedstorage: 200
          monitoringinterval: 1
      - type: mysql
        name: tprdevmysql6
        version: 5.7
        replicas: 3
        options:
          multiaz: true
          dbinstanceclass: db.m3.large
          allocatedstorage: 200
          monitoringinterval: 1
